{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
      "target": "ES2017",
      "module": "commonjs",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noUncheckedIndexedAccess": true, 
      "noEmit": true
    },
    "exclude": ["node_modules", "build", "dist"]
  }
  